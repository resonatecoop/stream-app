{
    "expo": {
        "name": "Resonate",
        "slug": "stream-app",
        "privacy": "unlisted",
        "version": "1.2.1",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.resonate.stream-app",
            "buildNumber": "1.2.1",
            "infoPlist": {
                "UIBackgroundModes": [
                    "audio",
                    "fetch"
                ]
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.resonate.streamapp",
            "versionCode": 121
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "expo-tracking-transparency",
                {
                    "userTrackingPermission": "Resonate only uses cookies to guarantee core functionalities, such as keeping you logged in for a while, or remembering your theme settings. No other type of activity is tracked. Visit https://resonate.is/cookie-policy to learn more, or view Resonate's open source code at https://github.com/resonatecoop."
                }
            ]
        ]
    }
}
